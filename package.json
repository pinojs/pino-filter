{
  "name": "pino-filter",
  "version": "1.0.0",
  "description": "A transport for Pino to filter logs based on logger names",
  "main": "index.js",
  "scripts": {
    "lint": "standard | snazzy",
    "test": "tap --no-cov 'test/**/*.test.js'",
    "test:ci": "standard | snazzy && tap \"test/**/*.test.js\" --coverage-report=lcovonly"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "bin": {
    "pino-filter": "./bin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pinojs/pino-filter.git"
  },
  "keywords": [
    "pino"
  ],
  "author": "James Sumners <james.sumners@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinojs/pino-filter/issues"
  },
  "homepage": "https://github.com/pinojs/pino-filter#readme",
  "devDependencies": {
    "flush-write-stream": "^2.0.0",
    "pre-commit": "^1.2.2",
    "snazzy": "^9.0.0",
    "standard": "^17.0.0",
    "tap": "^10.3.0"
  },
  "dependencies": {
    "deep-assign": "^3.0.0",
    "fast-json-parse": "^1.0.2",
    "pump": "^3.0.0",
    "split2": "^4.0.0",
    "through2": "^4.0.2"
  }
}
